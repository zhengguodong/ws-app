  align-items: center;
  flex-direction: column;
}
#mine #header {
  margin-top: 30px;
  width: 150px;
  height: 150px;
  background-color: pink;
  border-radius: 50%;
  overflow: hidden;
}
#mine #mid {
  margin-top: 5px;
  width: 100%;
  height: 50px;
